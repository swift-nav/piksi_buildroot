#!/bin/sh

# shellcheck disable=SC1091,SC2169,SC2039

name=feed_watchdog

watchdog_file=/dev/watchdog
watchdog_sleep=0.1

source /etc/init.d/common.sh

export log_tag=$name # required by logging.sh
source /etc/init.d/logging.sh

printf "V" >"$watchdog_file"

